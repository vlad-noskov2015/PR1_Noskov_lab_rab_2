        ymaps.ready(init); 
 
        function init() { 
            var myMap = new ymaps.Map('map', { 
                center: [-22.571583, 17.090736], 
                zoom: 8 
            }), 
                myRect = new ymaps.Rectangle([      
                    [-22.514206, 16.926084], 
                    [-22.680536, 17.460059]
                ]) 

               ;
                myMap.geoObjects.add(myRect) 
                .add(myGeoObject); 
        } 
